{
	"name": "@hlmpn/vite-plugin-iconify",
	"version": "2.0.0",
	"author": "hlmpn.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/hlpmenu/vite-plugin-iconify.git"
	},
	"scripts": {
		"build": "bun scripts/build.ts",
		"package:version": "bun scripts/echo_package.ts --version",
		"package:name": "bun scripts/echo_package.ts --name",
		"package:tag": "bun scripts/echo_package.ts --tag",
		"lint": "bun scripts/lint.ts --oxlint-only",
		"lint:fix": "bun scripts/lint.ts --fix",
		"format": "bunx --bun biome format --fix",
		"format:dry": "bunx --bun biome format",
		"biome:check": "bunx --bun biome check",
		"typecheck": "bunx --bun tsgo --noEmit -p ./tsconfig.json",
		"prepublishOnly": "bun run build"
	},
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"bugs": {
		"url": "https://github.com/hlpmenu/vite-plugin-iconify/issues"
	},
	"description": "A Vite plugin for dynamically inlining Iconify icons in Vue and React components.",
	"homepage": "https://github.com/hlpmenu/vite-plugin-iconify#readme",
	"keywords": [
		"vite",
		"plugin",
		"iconify",
		"icons",
		"vue",
		"react"
	],
	"license": "MIT",
	"dependencies": {
		"@babel/parser": "^7.28.5",
		"@babel/traverse": "^7.28.5",
		"@babel/types": "^7.28.5",
		"@nuxt/kit": "^4.2.1",
		"@nuxt/schema": "^4.2.1",
		"@rollup/pluginutils": "^5.3.0",
		"@types/babel__traverse": "^7.28.0",
		"@vue/compiler-sfc": "^3.5.24"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@types/bun": "^1.3.2",
		"@types/node": "^24.10.1",
		"@typescript/native-preview": "^7.0.0-dev.20251115.1",
		"esbuild": "^0.27.0",
		"oxlint": "^1.29.0",
		"oxlint-tsgolint": "^0.8.0",
		"rollup": "^4.53.3",
		"rollup-plugin-dts": "^6.2.3",
		"vite": "^7.2.2",
		"vue": "^3.5.24"
	}
}
