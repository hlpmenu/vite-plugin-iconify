{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "paths": {
      "*": [ "./*" ],
      "@hlmpn/vite-plugin-iconify": [ "../" ],
    },
    "module": "ESNext",
    "moduleResolution": "Bundler",

    "resolveJsonModule": true,
    "types": [ "node", "vite/client", "@nuxt/schema" ],
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,


  },
  "include": [
    "src/**/*.ts"
  ]
}
